# Core server configuration
DATABASE_URL=mysql://example-user:example-password@localhost:3306/main
JWT_SECRET=your-jwt-secret-change-in-production
OWNER_OPEN_ID=admin-open-id-optional

# OAuth configuration
# Server-side OAuth API base
OAUTH_SERVER_URL=https://api.manus.im
# Client-side OAuth portal (used to build login URL)
VITE_OAUTH_PORTAL_URL=https://oauth.manus.im
# Application ID registered with the OAuth portal
VITE_APP_ID=your-app-id

# OpenAI
OPENAI_API_KEY=your-openai-api-key

# Optional: UI branding (client only)
VITE_APP_TITLE="XAUUSD Prediction Agent"
VITE_APP_LOGO="https://placehold.co/128x128/E1E7EF/1F2937?text=XAU"

# Optional: Manus Forge API override (server only)
BUILT_IN_FORGE_API_URL=https://forge.manus.im
